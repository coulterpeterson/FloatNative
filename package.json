{
  "name": "floatnative",
  "private": true,
  "scripts": {
    "openapi:generate:swift": "pnpm --filter @floatnative/openapi generate:swift",
    "openapi:generate:kotlin": "pnpm --filter @floatnative/openapi generate:kotlin",
    "openapi:generate:all": "pnpm --filter @floatnative/openapi generate:all",
    "openapi:update-spec": "pnpm --filter @floatnative/openapi update-spec",
    "api:dev": "pnpm --filter api run dev",
    "api:deploy": "pnpm --filter api run deploy",
    "api:deploy:non-production": "pnpm --filter api run deploy:non-production",
    "api:tail": "pnpm --filter api run tail",
    "api:db:generate": "pnpm --filter api run db:generate",
    "api:db:migrate:local": "pnpm --filter api run db:migrate:local",
    "api:db:migrate:remote": "pnpm --filter api run db:migrate:remote"
    "extension:build": "pnpm --filter floatnative-chrome-extension run build",
    "extension:watch": "pnpm --filter floatnative-chrome-extension run watch"
  }
}