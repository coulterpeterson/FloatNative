{
  "name": "FloatNative-API",
  "version": "1.0.0",
  "description": "Companion API for Floatplane iOS/tvOS app - provides playlists and watch later functionality",
  "scripts": {
    "dev": "wrangler dev --env dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 execute floatnative-db-dev --local --env dev --file=./migrations/0000_curious_ink.sql",
    "db:migrate:remote": "wrangler d1 execute floatnative-db --remote --file=./migrations/0000_curious_ink.sql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.0",
  "dependencies": {
    "axios": "^1.13.1",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.9.2",
    "@types/uuid": "^11.0.0",
    "better-sqlite3": "^12.4.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "wrangler": "^4.50.0"
  }
}
